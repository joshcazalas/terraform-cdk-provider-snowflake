{
    "compilerOptions": {
      "outDir": "./dist/snowflake_resources",
      "declaration": true,
      "lib": ["ESNext", "dom"], 
      "target": "esNext", 
      "baseUrl": "./", 
      "esModuleInterop": true, 
      "moduleResolution": "node",
      "module": "CommonJS",
      "paths": {},
      "alwaysStrict": true,
      "allowUnreachableCode": false,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "noUncheckedIndexedAccess": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
    },
    "include": [
      "src/snowflake_resources/*.ts"
    ],
    "exclude": [
      "dist/**/*",
      "node_modules/**/*",
      "tools/**/*",
      "tools/*",
      "tools/get_parameters/*",
      "tools/create_resources/*",
      "main.ts"
    ]
  }
