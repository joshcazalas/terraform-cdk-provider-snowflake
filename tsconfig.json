{
    "compilerOptions": {
      "outDir": "./dist/snowflake_resources",
      // "declaration": true,
      "lib": ["ESNext", "dom"], 
      "target": "esNext", 
      "baseUrl": "./", 
      "esModuleInterop": true, 
      "moduleResolution": "node",
      "module": "CommonJS",
      "paths": {},
      "alwaysStrict": true,
      "allowUnreachableCode": false,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "noUncheckedIndexedAccess": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
    },
    "include": ["./**/*.ts"],
    "exclude": [
      "dist/**/*",
      "node_modules/**/*",
      // "tools/create_cdk_provider_files.ts",
      // "tools/create_resources_json.ts",
      // "tools/get_parameters/**",
    ]
  }